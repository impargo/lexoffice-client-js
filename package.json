{
  "name": "@impargo/lexoffice-client-js",
  "version": "1.2.1",
  "description": "An universal client for the Lexoffice API written in Typescript",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "umd:main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "commit": "cz",
    "build": "rm -rf dist && microbundle",
    "dev": "microbundle watch",
    "release": "npm run build && np",
    "lint": "eslint 'src/**/*.ts' --fix",
    "test": "echo \"No test specified\"",
    "start:example": "ts-node example/index.ts"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "yarn.lock"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "keywords": [
    "lexoffice",
    "api",
    "client",
    "javascript",
    "typescript",
    "node",
    "browser"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "elbstack <hello@elbstack.com>",
  "contributors": [
    "Annaelle RÃ¶ssert <annaelle@elbstack.com>",
    "David Paulus <d.paulus@impargo.de>"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.7.0",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "commitizen": "^4.3.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "local-package-publisher": "^1.0.4",
    "microbundle": "^0.13.1",
    "nock": "^13.0.7",
    "np": "^7.5.0",
    "prettier": "^2.2.1",
    "semantic-release": "^22.0.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "form-data": "^4.0.0",
    "ts-results": "^3.1.1",
    "tslib": "^2.2.0",
    "uri-tag": "^1.3.0",
    "utility-types": "^3.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
